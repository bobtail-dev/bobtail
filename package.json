{
  "name": "bobtail",
  "version": "1.8.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/bobtail-dev/bobtail.git"
  },
  "author": {
    "name": "Yang Zhang",
    "url": "http://yz.mit.edu/"
  },
  "main": "dist/reactive-coffee",
  "devDependencies": {
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coffee-preprocessor": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "build": "npm run clean-dist && npm run test && npm run copy && npm run minify",
    "clean-server": "rm -rf .tmp",
    "clean-dist": "rm -f dist/*",
    "coffee": "npm run rename && coffee -o .tmp -c --map .tmp/*.coffee && rm .tmp/*.coffee",
    "copy": "cp -ra .tmp/* dist",
    "karma": "if grep -qE \"(Microsoft|WSL)\" /proc/version; then export BOBTAIL_KARMA_BFW=1; fi; karma start karma.conf.js",
    "minify": "uglifyjs -mc --source-map -o dist/reactive-coffee.min.js dist/reactive-coffee.js",
    "rename": "cp src/reactive.coffee .tmp/reactive-coffee.coffee",
    "test": "npm run clean-server && mkdir -p .tmp && npm run coffee && npm run karma && npm run tsc",
    "tsc": "tsc --target es5 --strictNullChecks --alwaysStrict src/reactive.d.ts"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "jquery": ">=2.1.1",
    "uglify-es": "^3.0.23",
    "underscore": "^1.8.3"
  },
  "false": {}
}
