{
  "name": "bobtail",
  "license": "MIT",
  "version": "2.3.0-beta-0",
  "repository": {
    "type": "git",
    "url": "git://github.com/bobtail-dev/bobtail.git"
  },
  "author": {
    "name": "Yang Zhang",
    "url": "http://yz.mit.edu/"
  },
  "main": "dist/main.js",
  "contributors": [
    {
      "name": "Richard Mehlinger",
      "email": "rmehlinger@gmail.com",
      "url": "www.rmehlinger.com"
    },
    {
      "name": "Chung Wu"
    },
    {
      "name": "Joel Dodge"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.3.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-source-map-support": "^1.2.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.23"
  },
  "scripts": {
    "babel-test": "babel src/main.js --out-file .tmp/main.js --source-maps inline",
    "babel": "babel src/main.js --out-file dist/main.js --source-maps",
    "build": "npm run clean-dist && npm run test && npm run babel && npm run minify",
    "clean-server": "rm -rf .tmp",
    "clean-dist": "rm -f dist/*",
    "eslint": "eslint src/**/*.js --fix",
    "karma": "if grep -qE \"(Microsoft|WSL)\" /proc/version; then export BOBTAIL_KARMA_BFW=1; fi; karma start karma.conf.js",
    "minify": "uglifyjs -mc --source-map -o dist/main.min.js dist/main.js",
    "test": "npm run eslint && npm run clean-server && mkdir -p .tmp && npm run babel-test && npm run karma ; npm run tsc",
    "tsc": "tsc --target es5 --strictNullChecks --alwaysStrict src/main.d.ts"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "bobtail-rx": "2.3.0-beta-0",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "jquery": ">=2.1.1",
    "underscore": "^1.8.3"
  },
  "false": {}
}
